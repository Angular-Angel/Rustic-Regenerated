{
    "multipart": [{
            "when": { "type": "bottom|double", "facing": "north" },
            "apply": {"model": "rusticregen:block/crossed_logs", "y": 180}
        },
        {
            "when": { "type": "bottom|double", "facing": "east" },
            "apply": {"model": "rusticregen:block/crossed_logs", "y": 270}
        },
        {
            "when": { "type": "bottom|double", "facing": "south" },
            "apply": {"model": "rusticregen:block/crossed_logs"}
        },
        {
            "when": { "type": "bottom|double", "facing": "west" },
            "apply": {"model": "rusticregen:block/crossed_logs", "y": 90}
        },
        {
            "when": { "type": "top|double", "facing": "north" },
            "apply": {"model": "rusticregen:block/crossed_logs", "y": 90, "x": 180}
        },
        {
            "when": { "type": "top|double", "facing": "east" },
            "apply": {"model": "rusticregen:block/crossed_logs", "y": 180, "x": 180}
        },
        {
            "when": { "type": "top|double", "facing": "south" },
            "apply": {"model": "rusticregen:block/crossed_logs", "y": 270, "x": 180}
        },
        {
            "when": { "type": "top|double", "facing": "west" },
            "apply": {"model": "rusticregen:block/crossed_logs", "x": 180}
        },
        {
            "when": { "stake": "true" },
            "apply": {"model": "rusticregen:block/stake"}
        },
        {
            "when": { "leaves": "true" },
            "apply": {"model": "rusticregen:block/leaf_covering"}
        }
    ]
}