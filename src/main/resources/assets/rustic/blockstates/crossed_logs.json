{
    "multipart": [{
            "when": { "type": "bottom|double", "facing": "north" },
            "apply": {"model": "rustic:block/crossed_logs", "y": 180}
        },
        {
            "when": { "type": "bottom|double", "facing": "east" },
            "apply": {"model": "rustic:block/crossed_logs", "y": 270}
        },
        {
            "when": { "type": "bottom|double", "facing": "south" },
            "apply": {"model": "rustic:block/crossed_logs"}
        },
        {
